#################################################################
#    Classes
#################################################################

###  http://www.aes.org/ontologies/X098#ChannelType
:ChannelType rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty ec:hasIdentifier ;
                               owl:allValuesFrom ec:Identifier
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty ec:objectType ;
                               owl:allValuesFrom skos:Concept
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty ec:description ;
                               owl:allValuesFrom rdfs:Literal
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty ec:name ;
                               owl:allValuesFrom rdfs:Literal
                             ] ;
             rdfs:label "Channel type"@en .


###  http://www.aes.org/ontologies/X098#Device
:Device rdf:type owl:Class ;
        rdfs:subClassOf ccdm:ProductionDevice ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :functionalRole ;
                          owl:allValuesFrom skos:Consept
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasInputChannel ;
                          owl:allValuesFrom :DeviceChannel
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasOutputChanel ;
                          owl:allValuesFrom :DeviceChannel
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :settingsProfile ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass ec:Resource
                        ] ;
        dcterms:description "The device element shall be used to describe the setup and characteristics of a single device in the signal graph."@en ;
        rdfs:label "Device"@en .


###  http://www.aes.org/ontologies/X098#DeviceChannel
:DeviceChannel rdf:type owl:Class ;
               rdfs:subClassOf :ChannelType ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasDeviceParameter ;
                                 owl:someValuesFrom :DeviceParameter
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasDownstreamOutputChannel ;
                                 owl:allValuesFrom :DeviceChannel
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :interfaceType ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass skos:Concept
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :playbackMediaChannel ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:boolean
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :recordMediaChannel ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:boolean
                               ] ;
               dcterms:description "The DeviceChanel is an abstract class that serves as a base  for DeviceInputChanel and DeviceOutputChannel."@en ;
               rdfs:label "Device Channel"@en .


###  http://www.aes.org/ontologies/X098#DeviceInputChannel
:DeviceInputChannel rdf:type owl:Class ;
                    rdfs:subClassOf :DeviceChannel ;
                    owl:disjointWith :DeviceOutputChannel ;
                    dcterms:description "The DeviceInputChannel element shall be used to represent a single input connection on the device. One inputChannel element shall be present for each audio stream passing through a physical input connection that is used on the device."@en ;
                    rdfs:label "Device input channel"@en .


###  http://www.aes.org/ontologies/X098#DeviceOutputChannel
:DeviceOutputChannel rdf:type owl:Class ;
                     rdfs:subClassOf :DeviceChannel ;
                     dcterms:description "The DeviceOutputChannel element shall be used to represent a single output connection on the device. One DeviceOutputChannel shall be present for each audio stream passing through a physical output connection that is used on the device."@en ;
                     rdfs:label "Device output channel"@en .


###  http://www.aes.org/ontologies/X098#DeviceParameter
:DeviceParameter rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty :sendsToBus ;
                                   owl:allValuesFrom :DeviceChannel
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ec:hasIdentifier ;
                                   owl:allValuesFrom ec:Identifier
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ec:objectType ;
                                   owl:allValuesFrom skos:Concept
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :transitionDuration ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass ec:TimelinePoint
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :transitionType ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass skos:Concept
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ec:end ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass ec:TimelinePoint
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ec:start ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass ec:TimelinePoint
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ec:description ;
                                   owl:allValuesFrom rdfs:Literal
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ec:name ;
