###  http://www.aes.org/ontologies/X098#ProcessEvent
:ProcessEvent rdf:type owl:Class ;
              rdfs:subClassOf ecplus:ProductionJob ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :assuredBy ;
                                owl:allValuesFrom :QualityAssurance
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :feedsSideChainInput ;
                                owl:allValuesFrom :ProcessEvent
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasDownstreamOutput ;
                                owl:allValuesFrom :ProcessEvent
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasProcessEventGrupMember ;
                                owl:allValuesFrom :ProcessEvent
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasTimelineFractionProsessEvent ;
                                owl:allValuesFrom :ProcessEvent
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :reformattingReference ;
                                owl:allValuesFrom ec:Resource
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :trackReference ;
                                owl:allValuesFrom ecplus:Track
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :transitionDuration ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ec:TimelinePoint
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :transitionType ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass skos:Concept
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty ecplus:dateCreated ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass time:Instant
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty ecplus:end ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ec:TimelinePoint
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty ecplus:start ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ec:TimelinePoint
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :reformattingText ;
                                owl:allValuesFrom rdfs:Literal
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :inputFileStatusDeleted ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:boolean
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :sequenceNum ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:integer
                              ] ;
              dcterms:description """The ProcessEvent element shall be used to describe a single step of work involved in the restoration and/or preservation of an audio object. 

NOTE Each processEvent is bounded by the constraint that it shall have one or more input media objects and one or more output media objects. The event is therefore the documentation that describes what was done to effect the input media, to create the resulting output media. It is possible for the input and output media to refer to the same audioFileReference, adl, or audioObject element. For example, the action of cleaning a record, where both the input and output media of the event point to the same MediaObject describing the record."""@en ;
              rdfs:label "Process event"@en .


